{
  "AWS": {
    "Region": "us-east-1",
    "UseSecretsManager": true,
    "SecretName": "YOUR-AWS-SECRET-NAME",
    "DatabaseHost": "YOUR-AURORA-ENDPOINT.rds.amazonaws.com",
    "DatabasePort": 5432,
    "DatabaseName": "smartairport"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
      "Amazon.SecretsManager": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "YOUR-JWT-SECRET-KEY-HERE-MINIMUM-32-CHARACTERS",
    "Issuer": "FlightService",
    "Audience": "FlightServiceClients",
    "ExpiryMinutes": 60
  }
}
